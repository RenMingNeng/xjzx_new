	<from>^/teacher/list/([0-9]+).htm$</from>
	<to type="forward">/teacher/list.htm?id=$1</to>
</rule>
<rule>
	<note>培训资料库列表</note>
	<from>^/information/list/([0-9]+)/([0-9]+).htm$</from>
	<to type="forward">/information/list.htm?did=$1&amp;page=$2</to>
</rule>
<rule>
	<note>培训资料库详情</note>
	<from>^/information/detail/([0-9]+).htm$</from>
	<to type="forward">/information/detail.htm?id=$1</to>
</rule>
<rule>
	<note>法律法规列表</note>
	<from>^/law/list/([0-9]+)/([0-9]+).htm$</from>
	<to type="forward">/law/list.htm?dictid=$1&amp;page=$2</to>
</rule>
<rule>
	<note>法律法规详情</note>
	<from>^/law/detail/([0-9]+).htm$</from>
	<to type="forward">/law/detail.htm?id=$1</to>
</rule>
<rule>
	<note>安全生产月列表</note>
	<from>^/september/list/([0-9]+).htm$</from>
	<to type="forward">/september/list.htm?page=$1</to>
</rule>
<rule>
	<note>法律法规详情</note>
	<from>^/september/detail/([0-9]+).htm$</from>
	<to type="forward">/september/detail.htm?id=$1</to>
</rule>
<rule>
	<note>中心介绍详情</note>
	<from>^/normal/detail/([0-9]+).htm$</from>
	<to type="forward">/normal/detail.htm?pid=$1</to>
</rule>
<rule>
	<note>党建工作列表</note>
	<from>^/normal/list/([0-9]+)/([0-9]+).htm$</from>
	<to type="forward">/normal/list.htm?pid=$1&amp;page=$2</to>
</rule>
<rule>
	<note>党建工作列表</note>
	<from>^/normal/edit/([0-9]+).htm$</from>
	<to type="forward">/normal/edit.htm?id=$1</to>
</rule>
</urlrewrite>

