<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
 <rule>
	<note>新闻详情页面</note>
	<from>^/news/detail/([0-9]+).htm$</from>
	<to type="forward">/news/detail.htm?id=$1</to>
</rule>
<rule>
	<note>新闻类型列表2</note>
	<from>^/news/list/([0-9]+).htm$</from>
	<to type="forward">/news/list.htm?categoryid=$1&amp;page=1</to>
</rule>
<rule>
	<note>新闻类型列表1</note>
	<from>^/news/list/([0-9]+)/([0-9]+).htm$</from>
	<to type="forward">/news/list.htm?categoryid=$1&amp;page=$2</to>
</rule>

<rule>
	<note>全省监考人员名单</note>
	<from>^/proctor/list/([0-9]+).htm$</from>
	<to type="forward">/proctor/list.htm?id=$1</to>
</rule>
<rule>
	<note>全省安全培训师资名单</note>
	<from>^/evaluate/list/([0-9]+).htm$</from>
	<to type="forward">/evaluate/list.htm?id=$1</to>
</rule>
<rule>
